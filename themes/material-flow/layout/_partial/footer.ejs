<%
  let current = date(Date.now(), "YYYY");
%>
<footer id="footer" class="clearfix">
  <div class="social-wrapper">
    <% if (theme.social) { %>
      <% theme.social.forEach(function(value, key) {
        if (value.url && value.slug) { %>
        <a href="<%= value.url %>" class="social <%= value.slug %>"
          target="_blank" rel="external">
          <span class="<%= value.icon %>"></span>
        </a>
      <% } }); %>
    <% } %>
  </div>
  <div>
    Â© 2018 - <%= current %> <span itemprop="copyrightHolder"><%= config.author %></span>
  </div>
  <div>
    Powered by <a href="https://hexo.io/" class="codename" rel="external">Hexo</a> - Theme <a href='https://github.com/stkevintan/hexo-theme-material-flow' class="codename" rel="external">MaterialFlow</a>
  </div>
  <div>
    <a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/4.0/" rel="external">
      <img alt="Creative Commons License" style="border-width:0" src="https://i.creativecommons.org/l/by-nc-sa/4.0/80x15.png" />
    </a>
  </div>
</footer>

